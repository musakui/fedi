{
	"name": "@musakui/fedi",
	"description": "tools for the fediverse",
	"keywords": [
		"ActivityPub"
	],
	"author": "musakui",
	"license": "MIT",
	"repository": "github:musakui/fedi",
	"version": "0.1.1",
